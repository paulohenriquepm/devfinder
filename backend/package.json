{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Paulo Henrique",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "predev:server": "CONNECTION_NAME=default yarn typeorm migration:run -c default && yarn seed -c default",
    "dev:server": "CONNECTION_NAME=default ts-node-dev --transpile-only --ignore-watch node_modules src/app.ts",
    "prestart": "CONNECTION_NAME=default yarn typeorm migration:run -c default && yarn seed -c default",
    "start": "ts-node dist/server.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "pretest": "CONNECTION_NAME=test yarn typeorm migration:run -c test",
    "test": "CONNECTION_NAME=test jest",
    "posttest": "CONNECTION_NAME=test yarn typeorm migration:revert -c test"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.5.1",
    "ts-node": "^9.1.1",
    "typeorm": "^0.2.30",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "better-sqlite3": "^7.1.2",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
